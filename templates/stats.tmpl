{{template "top" $}}

<ul class="list-group">
  <li class="list-group-item">
    <span class="badge">{{$.Data.Imports}}</span>
    Imports Remaining
  </li>
  <li class="list-group-item">
    <span class="badge">{{$.Data.Days}}</span>
   Days Remaining
  </li>
</ul>


<div class="list-group">
  <a href="#" class="list-group-item active">Previous Imports</a>
  <table class="table table-striped table-hover ">
  <thead>
    <tr>
      <th>Date</th>
      <th>Zones</th>
      <th>Diff Duration</th>
      <th>Import Duration</th>
    </tr>
  </thead>
  <tbody>
  {{ range $key, $value := $.Data.Dates }}
    <tr>
      <td>{{date $value.Date}}</td>
      <td>{{$value.Count}}</td>
      <td>{{$value.DiffDuration}}</td>
      <td>{{$value.ImportDuration}}</td>
      
    </tr>
  {{ end }}
  </tbody>
  </table>
</div>
{{template "bottom" $}}
