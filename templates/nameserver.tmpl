      <td>{{if $value.LastSeen}}{{$value.LastSeen.Format "Jan 02, 2006"}}{{end}}</td>
    </tr>
  {{ end }}
    <tr>
  <td></td>
   <td></td>
      <td><span class="badge">{{if not $.Data.DomainCount}}{{if eq (len $.Data.Domains) 100}}100 of many{{else}}{{(len $.Data.Domains)}}{{end}}{{else}}{{$.Data.DomainCount}}{{end}}</span></td>
    </tr>
  </tbody>
  </table>
</div>
