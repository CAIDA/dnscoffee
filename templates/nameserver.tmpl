{{template "top" $}}
<div class="col-md-8" >
    <h1>{{$.Data.Name}}</h1>
    <p>
    {{if $.Data.FirstSeen}}{{$.Data.FirstSeen.Format "Jan 02, 2006"}}{{end}}
    -
    {{if $.Data.LastSeen}}{{$.Data.LastSeen.Format "Jan 02, 2006"}}{{end}}
    </p>
</div>
<div class="col-md-4" >
</div>
<div class="list-group col-md-6">
  <a href="#" class="list-group-item active">Current Domains<span class="badge">{{$.Data.DomainCount}}</span></a>
  <table class="table table-striped table-hover ">
  <thead>
    <tr>
      <th>Name</th>
      <th>Fist Seen</th>
    </tr>
  </thead>
  <tbody>
  {{ range $key, $value := $.Data.Domains }}
    <tr>
      <td><a href="/domains/{{$value.Name}}">{{$value.Name}}</a></td>
      <td>{{if $value.FirstSeen}}{{$value.FirstSeen.Format "Jan 02, 2006"}}{{end}}</td>
    </tr>
  {{ end }}
  </tbody>
  </table>
</div>
<div class="list-group col-md-6">
  <a href="#" class="list-group-item active">Past Domains<span class="badge">{{$.Data.ArchiveDomainCount}}</span></a>
  <table class="table table-striped table-hover ">
  <thead>
    <tr>
      <th>Name</th>
      <th>Fist Seen</th>
      <th>Last Seen</th>
    </tr>
  </thead>
  <tbody>
  {{ range $key, $value := $.Data.ArchiveDomains }}
    <tr>
      <td><a href="/domains/{{$value.Name}}">{{$value.Name}}</a></td>
      <td>{{if $value.FirstSeen}}{{$value.FirstSeen.Format "Jan 02, 2006"}}{{end}}</td>
      <td>{{if $value.LastSeen}}{{$value.LastSeen.Format "Jan 02, 2006"}}{{end}}</td>
    </tr>
  {{ end }}
  </tbody>
  </table>
</div>
{{template "bottom" $}}
