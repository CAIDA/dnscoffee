{{template "top" $}}
<div class="col-md-8" >
    <h1>{{$.Data.Name}}</h1>
    <p>
    {{date $.Data.FirstSeen}}
    -
    {{date $.Data.LastSeen}}
    </p>
</div>
<div class="col-md-4" >
</div>
<div class="list-group col-md-6">
  <a href="#" class="list-group-item active">Current Domains<span class="badge">{{count (len $.Data.Domains) $.Data.DomainCount}}</span></a>
  <table class="table table-striped table-hover ">
  <thead>
    <tr>
      <th>Name</th>
      <th>First Seen</th>
    </tr>
  </thead>
  <tbody>
  {{ range $key, $value := $.Data.Domains }}
    <tr>
      <td><a href="/domains/{{$value.Name}}">{{$value.Name}}</a></td>
      <td>{{date $value.FirstSeen}}</td>
    </tr>
  {{ end }}
  </tbody>
  </table>
</div>
<div class="list-group col-md-6">
  <a href="#" class="list-group-item active">Past Domains<span class="badge">{{count (len $.Data.ArchiveDomains) $.Data.ArchiveDomainCount}}</span></a>
  <table class="table table-striped table-hover ">
  <thead>
