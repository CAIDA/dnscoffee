{{template "top" $}}
<div class="col-md-8" >
    <h1>{{$.Data.Name}}</h1>
    <p>
    {{if $.Data.FirstSeen}}{{$.Data.FirstSeen.Format "Jan 02, 2006"}}{{end}}
    -
    {{if $.Data.LastSeen}}{{$.Data.LastSeen.Format "Jan 02, 2006"}}{{end}}
    </p>
</div>
<div class="col-md-4" >
</div>
<div class="list-group col-md-6">
  <a href="#" class="list-group-item active">Current Domains<span class="badge">{{if not $.Data.DomainCount}}{{if eq (len $.Data.Domains) 100}}100 of many{{else}}{{(len $.Data.Domains)}}{{end}}{{else}}{{$.Data.DomainCount}}{{end}}</span></a>
  <table class="table table-striped table-hover ">
  <thead>
    <tr>
      <th>Name</th>
      <th>First Seen</th>
    </tr>
  </thead>
  <tbody>
  {{ range $key, $value := $.Data.Domains }}
    <tr>
      <td><a href="/domains/{{$value.Name}}">{{$value.Name}}</a></td>
      <td>{{if $value.FirstSeen}}{{$value.FirstSeen.Format "Jan 02, 2006"}}{{end}}</td>
    </tr>
  {{ end }}
  <tr>
  <td></td>
      <td><span class="badge">{{if not $.Data.DomainCount}}{{if eq (len $.Data.Domains) 100}}100 of many{{else}}{{(len $.Data.Domains)}}{{end}}{{else}}{{$.Data.DomainCount}}{{end}}</span></td>
    </tr>
  </tbody>
  </table>
</div>
<div class="list-group col-md-6">
  <a href="#" class="list-group-item active">Past Domains<span class="badge">{{if not $.Data.ArchiveDomainCount}}{{if eq (len $.Data.ArchiveDomains) 100}}100 of many{{else}}{{(len $.Data.ArchiveDomains)}}{{end}}{{else}}{{$.Data.ArchiveDomainCount}}{{end}}</span></a>
